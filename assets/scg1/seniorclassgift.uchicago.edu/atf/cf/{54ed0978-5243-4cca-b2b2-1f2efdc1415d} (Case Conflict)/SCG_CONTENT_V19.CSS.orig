#stripes { background-image:url('/atf/cf/{54ed0978-5243-4cca-b2b2-1f2efdc1415d}/stripe.gif'); background-repeat:repeat ; height: 20px; }

/*-------------------------------------------------
   Progress Chart
-------------------------------------------------*/

#scg-progress {
width: 220px;
height: 300px;
text-align: center;
background-image: url(http://seniorclassgift.uchicago.edu/atf/cf/%7B54ed0978-5243-4cca-b2b2-1f2efdc1415d%7D/SCG-CHART-2014.GIF);
}
#scg-thermometer {
width: 100px;
height: 100%;
margin-right: auto;
margin-left: auto;
margin-bottom: 0px;
padding-bottom: 0px;
z-index: 10;
}
#scg-highchart {
position: inherit;
top: 0px;
width: 100px;
height: 100%;
margin-right: auto;
margin-left: auto;
margin-bottom: 0px;
padding-bottom: 0px;
z-index: 0px
}
#scg-empty {
width: 100px;
height: 100%;
margin: 0px;
}
#scg-mercury {
width: 100px;
height: 0%;
