@charset "UTF-8";
/* CSS Document */

/*-- Content Style ------------------------------------------------------------------------------------------------*/

/* Global */
body { font-family: Arial, Helvetica, sans-serif; font-size: 11px; color: #474740; margin: 0px }


/*-------------------------------------------------
   Typography
-------------------------------------------------*/

p { font-family: Arial, Helvetica, sans-serif; font-size: 11px; color: #474740; line-height:1.5em; }
a { font-family: Arial, Helvetica, sans-serif; font-size: 11px; color: #155F83; text-decoration: none; } a:hover { text-decoration: underline; }
h1 { font-family: Arial, Helvetica, sans-serif; font-size: 24px; color: #c66717; font-weight:bold; margin: 0px 0px 16px 0px; padding: 0px; text-transform:uppercase; }
h2 { font-family: Arial, Helvetica, sans-serif; font-size: 16px; color: #534f44; font-weight:bold; margin: 0px 0px 16px 0px; padding: 0px; }
h3 { font-family: Arial, Helvetica, sans-serif; font-size: 11px; color: #534F44; text-transform:uppercase; font-weight:bold; border-bottom: ; padding-bottom:2px; margin-top:8px }
h4 { font-family: Arial, Helvetica, sans-serif; font-size: 15px; color: #902626; margin:0px; font-weight:bold }


#event h1 { font-family:Arial, Helvetica, sans-serif; font-size: 18px; color: #c66717; margin: 0; padding: 0px; font-weight:bold; text-transform:uppercase;  }
#event h2 { font-family:Arial, Helvetica, sans-serif; font-size: 13px; color: #534f44; margin: 0 0 3% 0; padding: 0px; font-weight:bold; text-transform:uppercase; }
#event h2 a { color: #155F83; text-decoration: none; font-size: 13px;}

/* Lists */

#right_col ul  { padding: 0 0 0 1.5em; margin: 0 0 20px 0; } 
#right_col li { list-style:square url("/atf/cf/{5ecc41d5-fd4d-40f6-bb95-3e38333a1b0b}/BULLET.GIF"); padding-left: 0; margin-bottom: .75em; }

#main_col ul  { padding: 0 0 0 1.5em; margin: 0 24px 20px 20px; } 
#main_col li { list-style:square url("/atf/cf/{5ecc41d5-fd4d-40f6-bb95-3e38333a1b0b}/BULLET.GIF"); padding-left: 0; margin-bottom: .75em; line-height:1.5em; }

ul#short_list  { padding: 0 0 0 1.5em; margin: 0; } 
ul#short_list li { list-style:square url("/atf/cf/{5ecc41d5-fd4d-40f6-bb95-3e38333a1b0b}/BULLET.GIF"); padding-left: 0; margin-bottom: .5em; }

#right_col_one h2 { margin-top:0px }
#main_col h3 { border-bottom: 1px solid #C9C3A3; }

hr {color: #fff; background-color: #fff; border: 2px dotted #534f44; border-style: none none dotted; }

