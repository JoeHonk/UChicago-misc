            <script>
            (function($) {
                $(function() {

                    $('#jump-to-nav').click(function(e) {
                        e.preventDefault();
                        $(this).toggleClass('active');
                        $('#nav').toggleClass('active');
                    });

                    $('#nav .close').click(function(e) {
                        $('#jump-to-nav').removeClass('active');
                        $('#nav').removeClass('active');
                        return false;
                    });

                    $('.has-submenu', '#nav').on('click', function(e) {
                        if (!$(e.target).is("a")) {
                            $(this).toggleClass('active');
                            return false;
                        }
                    });

                    $('a', '#nav').on('click', function(e) {
                        e.stopPropagation();
                    });

                    $(document).on('click', function(e) {
                        $('.has-submenu.active').removeClass('active');
                    });

                });
            })(jQuery);
            </script>
